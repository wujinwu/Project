<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  设置背景色  -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8dp" />
            <gradient
                android:angle="0"
                android:endColor="#F2F3F8"
                android:startColor="#F2F3F8" />

        </shape>
    </item>

    <!--  设置进度条颜色  -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8dp" />
                <!--//设置开始的颜色-->
                <!--//设置变到的颜色-->
                <!--//设置从哪个方向-->
                <gradient
                    android:angle="45"
                    android:endColor="@color/c_801296DB"
                    android:startColor="@color/c_801296DB" />
            </shape>
        </clip>
    </item>
</layer-list>